{
  "name": "npm-es-modules-5-rollup",
  "version": "0.0.2",
  "description": "Breakdown of 7 different ways to use ES modules with NPM today.",
  "main": "dist/main.js",
  "module": "dist/module.mjs",
  "browser": "dist/browser.js",
  "repository": "transitive-bullshit/npm-es-modules",
  "author": "Travis Fischer <travis@automagical.ai>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "yarn run build:node && yarn run build:browser",
    "build:node": "rollup -c rollup.node.js",
    "build:browser": "rollup -c rollup.browser.js",
    "start": "rollup -c rollup.node.js -w",
    "clean": "del dist",
    "prepublishOnly": "yarn run build",
    "pretest": "yarn run build",
    "test": "ava -v && standard"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "npm",
    "es",
    "esm",
    "rollup",
    "babel",
    "modules",
    "commonjs"
  ],
  "devDependencies": {
    "ava": "^1.0.0-beta.5.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "del-cli": "^1.1.0",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "standard": "^11.0.0"
  },
  "dependencies": {
    "get-pixels": "^3.3.0",
    "ow": "^0.4.0"
  },
  "ava": {
    "babel": false
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers",
      "transform-runtime"
    ]
  }
}
